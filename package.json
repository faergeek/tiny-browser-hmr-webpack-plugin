{
  "private": true,
  "main": "build/main.js",
  "scripts": {
    "prettier": "prettier --write .",
    "prettier:check": "prettier --list-different .",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "tsc",
    "dev": "webpack watch --node-env=development",
    "build": "webpack --node-env=production",
    "build:analyze": "webpack watch --node-env=production",
    "cleanup": "rimraf build",
    "start": "node --enable-source-maps ."
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "eslintConfig": {
    "root": true,
    "extends": "@faergeek",
    "overrides": [
      {
        "files": "*",
        "extends": "@faergeek/eslint-config/node"
      },
      {
        "files": "src/**/*",
        "extends": "@faergeek/eslint-config/preact"
      },
      {
        "files": "**/*.ts?(x)",
        "extends": "@faergeek/eslint-config/typescript"
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "not IE <= 11",
    "not OperaMini all",
    "not dead"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "compression": "^1.7.4",
    "core-js": "^3.2.1",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "tiny-invariant": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.13.0",
    "@faergeek/eslint-config": "^5.0.0",
    "@prefresh/babel-plugin": "^0.4.4",
    "@prefresh/core": "^1.4.1",
    "@prefresh/webpack": "^3.3.4",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.12",
    "babel-loader": "^9.0.0",
    "browserslist": "^4.16.6",
    "eslint": "^8.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "source-map-loader": "^4.0.1",
    "ssestream": "^1.1.0",
    "typescript": "^4.3.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.9.1"
  }
}
