{
  "private": true,
  "scripts": {
    "cleanup": "rimraf build",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --list-different .",
    "lint": "eslint --max-warnings 0 .",
    "dev": "webpack watch --node-env=development",
    "build": "webpack --node-env=production",
    "start": "http-server build -c-1 -a localhost"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "eslintConfig": {
    "root": true,
    "extends": "@faergeek",
    "overrides": [
      {
        "files": "*",
        "extends": "@faergeek/eslint-config/node"
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "not IE <= 11",
    "not OperaMini all",
    "not dead"
  ],
  "dependencies": {
    "ssestream": "^1.1.0"
  },
  "devDependencies": {
    "@faergeek/eslint-config": "^5.0.0",
    "eslint": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.9.1"
  }
}
